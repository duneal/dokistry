{
  "name": "dokistry",
  "description": "Dokistry is an open-source tool to preview and manage Docker registries, images, and users from a single interface.",
  "version": "0.1.0",
  "author": "Duneal Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/duneal/dokistry.git"
  },
  "bugs": {
    "url": "https://github.com/duneal/dokistry/issues"
  },
  "homepage": "https://github.com/duneal/dokistry#readme",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "start:dev": "next dev --turbopack",
    "start:prod": "next start",
    "build": "next build",
    "lint": "biome check && tsc --noEmit",
    "lint:fix": "biome check --fix && tsc --noEmit",
    "format": "biome format --write .",
    "semantic-release": "semantic-release",
    "db:generate": "DATABASE_URL='postgresql://postgres:postgres@127.0.0.1:5432/dokistry' drizzle-kit generate",
    "db:migrate": "DATABASE_URL='postgresql://postgres:postgres@127.0.0.1:5432/dokistry' drizzle-kit migrate",
    "db:push": "DATABASE_URL='postgresql://postgres:postgres@127.0.0.1:5432/dokistry' drizzle-kit push",
    "db:studio": "DATABASE_URL='postgresql://postgres:postgres@127.0.0.1:5432/dokistry' drizzle-kit studio --host=127.0.0.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "biome check --fix"
    ]
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@re-dev/react-truncate": "^0.4.4",
    "@tanstack/react-query": "^5.75.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/pg": "^8.15.5",
    "antd": "^5.25.2",
    "axios": "^1.7.7",
    "better-auth": "^1.3.16",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.4",
    "drizzle-orm": "^0.44.5",
    "lucide-react": "^0.511.0",
    "next": "^15.3.1",
    "postgres": "^3.4.7",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^19.1.1",
    "react-loading-skeleton": "^3.5.0",
    "react-responsive": "^10.0.0",
    "recharts": "^3.2.1",
    "slugify": "^1.6.6",
    "sonner": "^2.0.7",
    "swiper": "^11.1.15",
    "truncate": "^3.0.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@qmweb/next-maker": "^1.0.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@svgr/webpack": "^8.1.0",
    "@types/leaflet": "^1.9.16",
    "@types/lunr": "^2.3.7",
    "@types/node": "22.14.0",
    "@types/qs": "^6",
    "@types/react": "^18.3.9",
    "@types/react-is": "^19",
    "drizzle-kit": "^0.31.4",
    "lefthook": "^1.11.12",
    "lint-staged": "^15.5.0",
    "sass": "^1.83.0",
    "semantic-release": "^24.0.0",
    "typescript": "^5.6.2"
  }
}
